{
    "name": "amaccis/tai64-datetime",
    "type": "library",
    "description": "A PHP library that extends DateTime class, in order to allow to handle Tai64. ",
    "keywords": [
        "tai64",
        "tai64n",
        "tai64na",
        "date",
        "time",
        "datetime"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrea Maccis",
            "email": "andrea.maccis@gmail.com"
        }
    ],
    "require": {
		"php": "^8.1",
        "ext-ctype": "*"
    },
    "autoload": {
        "psr-4": {
            "Amaccis\\Tai64\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Amaccis\\Tai64\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    }
}
